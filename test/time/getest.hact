test

	xtime

default

	class ("DT_TEST_[A-Z0-9_]*")
	feature ("test_[a-z0-9_]*")
	prefix ("X")
	testgen ("TESTGEN")
	compile ("bash -c %" ( ( echo q | ibcomp -executable -new -ace hact.ace -project xtime.eif ) ; cd xtime_gen/W_code ; fish ) > tmp_compile.txt 2>&1%"")
	execute ("bash -c %"cd xtime_gen/W_code ; ./xtime%"")

cluster

	time: "$GOBO/test/time"

end
