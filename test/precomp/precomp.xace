<?xml version="1.0"?>

<cluster name="precomp" abstract="True">
	<description>
		system:     "Gobo Eiffel Libraries precompilation"
		author:     "Eric Bezault [ericb@gobosoft.com]"
		copyright:  "Copyright (c) 1999-2001, Eric Bezault and others"
		license:    "Eiffel Forum Freeware License v1 (see forum.txt)"
		date:       "$Date$"
		revision:   "$Revision$"
	</description>

	<mount location="${GOBO}/library/kernel/cluster.xace"/>
	<mount location="${GOBO}/library/lexical/cluster.xace"/>
	<mount location="${GOBO}/library/parse/cluster.xace"/>
	<mount location="${GOBO}/library/pattern/cluster.xace"/>
	<mount location="${GOBO}/library/structure/cluster.xace"/>
	<mount location="${GOBO}/library/test/cluster.xace"/>
	<mount location="${GOBO}/library/time/cluster.xace"/>
	<mount location="${GOBO}/library/tools/cluster.xace"/>
	<mount location="${GOBO}/library/utility/cluster.xace"/>
	<mount location="${GOBO}/library/xml/cluster.xace"/>

	<cluster name="EiffelTime" location="${GOBO}/library/other/EiffelTime" if="${GOBO_EIFFEL}=ise"/>
	<cluster name="EiffelBase" location="${GOBO}/library/other/EiffelBase" if="${GOBO_EIFFELBASE}"/>
	<mount location="${GOBO}/library/kernel.xace">
		<exclude cluster="kernel.ise.base.structures" if="${GOBO_EIFFELBASE}"/>
	</mount>
</cluster>
