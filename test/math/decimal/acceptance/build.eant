<?xml version="1.0"?>

<project name="decimal" default="help">

	<description>
		system: "Gobo Eiffel Math Library Tester"
		copyright: "Copyright (c) 2005, Eric Bezault and others"
		license: "Eiffel Forum License v2 (see forum.txt)"
		date: "$Date$"
		revision: "$Revision: "
	</description>

	<inherit>
		<parent location="${GOBO}/misc/test.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>


<!-- Implementation -->

	<target name="init_system">
		<set name="system" value="xdecimal"/>
		<set name="library" value="math"/>
		<set name="system_dir" value="${GOBO}/test/${library}/decimal"/>
	</target>

</project>
