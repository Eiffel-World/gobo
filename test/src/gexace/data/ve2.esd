system

	xace2

root

	FOO

creation

	make

cluster

	[gefoo "$GOBO/test/src/gexace/data"] end

option

	assertions on
	require off
	ensure off
	class invariant off
	loop invariant off
	loop variant off
	check off
	optimize dead_code on
	optimize calls on
	optimize dyn_type on
	debug instructions off
	debug off
	run_time_in_dll off
	finalize off
	optimize flat_fst off
	FST_expansion_factor 2
	optimize FST off
	optimize inline on
	optimize constants on
	optimize once on
	optimize jumps off
	optimize layout off
	optimize leaves off
	linker default
	map off
	no_default_lib off
	optimize reloads off
	target exe console

end
