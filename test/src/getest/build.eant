<?xml version="1.0"?>

<project name="getest" inherit="${GOBO}/misc/test.eant" default="help">

	<description>
		system: "Gobo Eiffel Test Tester"
		copyright: "Copyright (c) 2001, Eric Bezault and others"
		license: "Eiffel Forum License v1 (see forum.txt)"
		date: "$Date$"
		revision: "$Revision$"
	</description>


<!-- Implementation -->

	<target name="other_clean" depend="init">
		<description>
			Remove other intermediary generated files.
		</description>

		<delete directory="Tgetest"/>
	</target>

	<target name="init_system">
		<set name="system" value="xgetest"/>
		<set name="tool" value="getest"/>
		<set name="system_dir" value="${GOBO}/test/src/${tool}"/>
	</target>

</project>
