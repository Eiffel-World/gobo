<?xml version="1.0"?>

<library name="time" prefix="dt_">
	<description>
		description: "Xace file for the Gobo Eiffel Time Library"
		library: "Gobo Eiffel Time Library"
		copyright: "Copyright (c) 2001, Eric Bezault and others"
		license: "Eiffel Forum License v2 (see forum.txt)"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<cluster name="time" location="${GOBO}/library/time">
		<option name="abstract" value="true"/>
		<cluster name="clock"/>
		<cluster name="date"/>
		<cluster name="format"/>
		<cluster name="time_zone"/>
	</cluster>
</library>
