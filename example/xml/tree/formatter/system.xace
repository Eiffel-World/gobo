<?xml version="1.0"?>
<system name="formatter">
	<root class="FORMATTER" creation="make"/>
	<cluster>
		<option>
			<require enable="True"/>
			<ensure enable="True"/>
		</option>

		<cluster name="formatter" location="${GOBO}/example/xml/tree/formatter"/>

		<mount location="${GOBO}/library/cluster.xace">
			<exclude cluster="gobo.xml.impl.expat"/>
			<exclude cluster="gobo.xml.impl.no_eiffel"/>
			<exclude cluster="gobo.xml.impl.no_tree_on_event"/>
		</mount>

		<mount location="${GOBO}/library/kernel.xace" />
	</cluster>
</system>
