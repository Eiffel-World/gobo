	[ds_table			"$GOBO\library\structure\table"] end
	[ds_table_spec		"$GOBO\library\structure\table\spec\ve"] end

		-- Visual Eiffel Kernel
	[Kernel				"$VE_LIB\kernel"] end

option

	target exe console
	code native

end
